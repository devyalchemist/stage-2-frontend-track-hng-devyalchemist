{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["views/**", "public/assets/**"]
			}
		}
	],
	"installCommand": "npm install",
	"buildCommand": "npm run build:css",
	"routes": [
		{
			"src": "/(.*)",
			"dest": "server.js"
		}
	]
}
